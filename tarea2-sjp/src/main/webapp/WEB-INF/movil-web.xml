<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xmlns="https://jakarta.ee/xml/ns/jakartaee" 
         xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" 
         metadata-complete="false" 
         version="5.0">
  <display-name>EventosUY - Aplicación Móvil</display-name>
  
  <!-- Context Parameters for Web Services Configuration -->
  <context-param>
    <description>URL del servidor de Web Services</description>
    <param-name>webservices.url</param-name>
    <param-value>http://localhost:9128/webservices</param-value>
  </context-param>
  
  <!-- Welcome Files -->
  <welcome-file-list>
    <welcome-file>movil-index.jsp</welcome-file>
  </welcome-file-list>
  
  <!-- Error Pages -->
  <error-page>
    <error-code>404</error-code>
    <location>/WEB-INF/shared/errorPages/404.jsp</location>
  </error-page>
  
  <error-page>
    <error-code>500</error-code>
    <location>/WEB-INF/shared/errorPages/500.jsp</location>
  </error-page>
</web-app>
